            "font_style": null,
            "font_weight": null
          },
          "constant": {
            "color": "#ff9e64",
            "font_style": null,
            "font_weight": null
          },
          "constructor": {
            "color": "#f7768e",
            "font_style": null,
            "font_weight": null
          },
          "emphasis": {
            "color": "#f7768e",
            "font_style": "italic",
            "font_weight": null
          },
          "emphasis.strong": {
            "color": "#f7768e",
            "font_style": null,
            "font_weight": 700
